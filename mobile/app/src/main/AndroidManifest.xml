<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.toeic">
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:name=".config.ApplicationConfig"
        android:allowBackup="true"
        android:icon="@drawable/unnamed"
        android:label="@string/app_name"
        android:roundIcon="@drawable/unnamed"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name=".feature.home.HomeActivity" />
        <activity
            android:name=".feature.login.LoginActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".feature.exam.part_one.PartOneActivity" />
        <activity android:name=".feature.exam.part_one_exam.PartOneExamActivity" />
        <activity android:name=".feature.exam.part_two.PartTwoActivity" />
        <activity android:name=".feature.exam.part_two_exam.PartTwoExamActivity" />
        <activity android:name=".feature.exam.part_three.PartThreeActivity" />
        <activity android:name=".feature.exam.part_three_exam.PartThreeExamActivity" />
        <activity android:name=".feature.exam.part_four.PartFourActivity" />
    </application>

</manifest>